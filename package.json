{
  "name": "xebia-assignment",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "^9.7.3",
    "axios": "^0.19.0",
    "cypress": "^3.7.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "tailwindcss": "^1.1.4",
    "typescript": "3.7.2",
    "yup": "^0.27.0"
  },
  "scripts": {
    "start": "concurrently \"npm run start:twd\" \"npm run start:cra\"",
    "build": "npm run build:twd && npm run build:cra",
    "start:cra": "react-scripts start",
    "build:cra": "react-scripts build",
    "start:twd": "postcss ./src/styles/tailwind.pcss -o ./src/styles/tailwind.css --watch",
    "build:twd": "postcss ./src/styles/tailwind.pcss -o ./src/styles/tailwind.css --env production",
    "test": "react-scripts test",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "24.0.23",
    "@types/node": "12.12.14",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "concurrently": "^5.0.0",
    "postcss-cli": "^6.1.3"
  }
}
